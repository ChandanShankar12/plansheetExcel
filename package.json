{
  "name": "excel-processor",
  "version": "1.0.0",
  "type": "module",
  "author": "Jane Doe",
  "license": "MIT",
  "main": "electron/dist/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "electron": "tsc -p electron/tsconfig.json && electron .",
    "electron-dev": "tsc -p electron/tsconfig.json && cross-env NODE_ENV=development electronmon .",
    "electron-build": "next build && tsc -p electron/tsconfig.json && electron-builder",
    "dev:all": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run electron-dev\"",
    "db:generate": "dotenv -e .env.local drizzle-kit generate:pg",
    "db:push": "dotenv -e .env.local drizzle-kit push:pg",
    "db:studio": "dotenv -e .env.local drizzle-kit studio",
    "db:seed": "dotenv -e .env.local tsx src/lib/db/seed.ts"
  },
